{
  "name": "down-the-code",
  "description": "Using Gatsby default starter, building a website that displays pictures of travels",
  "version": "1.0.0",
  "author": "Raja Bellebon <rbellebon@gmail.com>",
  "dependencies": {
    "@babel/preset-env": "^7.12.1",
    "@emotion/react": "^11.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "gatsby": "^3.1.0",
    "gatsby-image": "^3.1.0",
    "gatsby-plugin-emotion": "^6.1.0",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-sharp": "^3.1.0",
    "gatsby-plugin-testing": "^0.3.4",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-transformer-remark": "^3.1.0",
    "gatsby-transformer-sharp": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "reactstrap": "^7.1.0"
  },
  "keywords": [
    "down-the-code",
    "tulec"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:e2e:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test develop 8000 test:e2e",
    "test:e2e:watch": "cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/reactstrap": "^8.7.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-gatsby": "^0.1.6",
    "cypress": "^6.8.0",
    "dotenv": "^6.2.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "react-media": "^1.9.2",
    "start-server-and-test": "^1.10.2",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write \"src/**/*.{js,ts,tsx}\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RajaBellebon/tulec"
  }
}
